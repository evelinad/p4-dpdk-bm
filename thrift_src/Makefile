gen-cpp/bm/simple_pre_lag_types.lo: gen-cpp/bm/$(am__dirstamp)

libruntimestubs.la: $(libruntimestubs_la_OBJECTS) $(libruntimestubs_la_DEPENDENCIES) $(EXTRA_libruntimestubs_la_DEPENDENCIES) 
	$(AM_V_CXXLD)$(CXXLINK) -rpath $(libdir) $(libruntimestubs_la_OBJECTS) $(libruntimestubs_la_LIBADD) $(LIBS)

mostlyclean-compile:
	-rm -f *.$(OBJEXT)
	-rm -f gen-cpp/bm/*.$(OBJEXT)
	-rm -f gen-cpp/bm/*.lo

distclean-compile:
	-rm -f *.tab.c

.cpp.o:
	$(AM_V_CXX)$(CXXCOMPILE) -c -o $@ $<

.cpp.obj:
	$(AM_V_CXX)$(CXXCOMPILE) -c -o $@ `$(CYGPATH_W) '$<'`

.cpp.lo:
	$(AM_V_CXX)$(LTCXXCOMPILE) -c -o $@ $<

mostlyclean-libtool:
	-rm -f *.lo

clean-libtool:
	-rm -rf .libs _libs
	-rm -rf gen-cpp/bm/.libs gen-cpp/bm/_libs
install-nodist_rootpyPYTHON: $(nodist_rootpy_PYTHON)
	@$(NORMAL_INSTALL)
	@list='$(nodist_rootpy_PYTHON)'; dlist=; list2=; test -n "$(rootpydir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(rootpydir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(rootpydir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then b=; else b="$(srcdir)/"; fi; \
	  if test -f $$b$$p; then \
	    $(am__strip_dir) \
	    dlist="$$dlist $$f"; \
	    list2="$$list2 $$b$$p"; \
	  else :; fi; \
	done; \
	for file in $$list2; do echo $$file; done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(rootpydir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(rootpydir)" || exit $$?; \
	done || exit $$?; \
	if test -n "$$dlist"; then \
	  $(am__py_compile) --destdir "$(DESTDIR)" \
	                    --basedir "$(rootpydir)" $$dlist; \
	else :; fi

uninstall-nodist_rootpyPYTHON:
	@$(NORMAL_UNINSTALL)
	@list='$(nodist_rootpy_PYTHON)'; test -n "$(rootpydir)" || list=; \
	py_files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$py_files" || exit 0; \
	dir='$(DESTDIR)$(rootpydir)'; \
	pyc_files=`echo "$$py_files" | sed 's|$$|c|'`; \
	pyo_files=`echo "$$py_files" | sed 's|$$|o|'`; \
	py_files_pep3147=`echo "$$py_files" | $(am__pep3147_tweak)`; \
	echo "$$py_files_pep3147";\
	pyc_files_pep3147=`echo "$$py_files_pep3147" | sed 's|$$|c|'`; \
	pyo_files_pep3147=`echo "$$py_files_pep3147" | sed 's|$$|o|'`; \
	st=0; \
	for files in \
	  "$$py_files" \
	  "$$pyc_files" \
	  "$$pyo_files" \
	  "$$pyc_files_pep3147" \
	  "$$pyo_files_pep3147" \
	; do \
	  $(am__uninstall_files_from_dir) || st=$$?; \
	done; \
	exit $$st
